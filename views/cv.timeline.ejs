<% include cv.header.ejs %>
	<main id="main">
		<div class="container">
			<div class="timeline fin">
<% timeline.forEach( function(data) { %>
				<div class="item">
					<div class="node">
					</div>
					<div class="title">
						<span class="t1"><small><%= data.title || "" %></small></span>
						<div class="time"><%= data.time || "" %></div>
<% if(data.detail.length>0) { %>
						<small class="list">
							<ul>
<% data.detail.forEach( function(row) { %>
								<li><%-row%></li>
<% }) %>
							</ul>
						</small>
<% } %>
					</div>
				</div>
<%	}) %>
			</div>
			<div class="gap"></div>
			<div class="gap"></div>
		</div>
	</main>
<% include cv.footer.ejs %>